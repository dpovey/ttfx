{
  "name": "my-typesugar-macros",
  "version": "1.0.0",
  "description": "Custom macros for typesugar",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./macros": {
      "types": "./dist/macros/index.d.ts",
      "import": "./dist/macros/index.js",
      "require": "./dist/macros/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "ts-patch install -s"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "typesugar",
    "macro",
    "typescript"
  ],
  "dependencies": {
    "@typesugar/core": "^0.1.0"
  },
  "devDependencies": {
    "@typesugar/transformer": "^0.1.0",
    "@typesugar/testing": "^0.1.0",
    "ts-patch": "^3.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "@typesugar/transformer": ">=0.1.0",
    "typescript": ">=5.0.0"
  }
}
