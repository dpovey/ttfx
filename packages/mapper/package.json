{
  "name": "@typesugar/mapper",
  "version": "0.1.0",
  "description": "Zero-cost compile-time object mapping and transformation",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "typescript",
    "macros",
    "mapping",
    "transform",
    "automapper"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@typesugar/core": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "files": [
    "dist",
    "src"
  ]
}
