{
  "name": "@typesugar/preprocessor",
  "version": "0.1.0",
  "description": "Lexical preprocessor for typesugar syntax extensions (HKT, custom operators)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "preprocessor",
    "hkt",
    "operators",
    "lexer"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "magic-string": "^0.30.21"
  }
}
