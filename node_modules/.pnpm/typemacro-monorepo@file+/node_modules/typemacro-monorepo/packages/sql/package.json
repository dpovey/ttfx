{
  "name": "@typemacro/sql",
  "version": "0.1.0",
  "description": "Doobie-like type-safe SQL DSL with compile-time query building for typemacro",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "typescript",
    "macros",
    "sql",
    "database",
    "doobie",
    "query-builder"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@typemacro/core": "workspace:*"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "files": [
    "dist",
    "src"
  ]
}
